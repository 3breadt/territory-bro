{
  "presets": [
    [
      "env", {
      "targets": {"browsers": ["last 2 versions"]},
      "modules": false,
      "useBuiltIns": true,
      "debug": false
    }
    ],
    "react",
    "flow"
  ],
  "plugins": [
    "transform-object-rest-spread",
    ["react-intl", {"messagesDir": "./target/messages/"}]
  ],
  "env": {
    "test": {
      "presets": [
        ["env", {"targets": {"node": "current"}}],
        "react",
        "flow"
      ]
    }
  }
}
