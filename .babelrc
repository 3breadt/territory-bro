{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "useBuiltIns": "usage",
      "corejs": 3,
      "debug": false
    }],
    "@babel/react",
    "@babel/flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-throw-expressions",
    ["react-intl", {
      "messagesDir": "./target/messages/",
    }]
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {"node": true}
        }],
        "@babel/react",
        "@babel/flow"
      ]
    }
  }
}
