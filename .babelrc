{
  "presets": [
    [
      "@babel/preset-env", {
      "targets": {"browsers": ["last 2 versions"]},
      "modules": false,
      "useBuiltIns": "entry",
      "debug": false
    }
    ],
    "@babel/react",
    "@babel/flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    ["react-intl", {"messagesDir": "./target/messages/"}]
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/env", {"targets": {"node": "current"}}],
        "@babel/react",
        "@babel/flow"
      ]
    }
  }
}
