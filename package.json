{
  "name": "territory-bro",
  "version": "1.0.0-SNAPSHOT",
  "description": "Territory Bro is a tool for managing territory cards in the congregations of Jehovah's Witnesses.",
  "homepage": "https://territorybro.com",
  "author": "Esko Luontola (http://luontola.fi)",
  "license": "Apache-2.0",
  "bugs": "https://github.com/orfjackal/territory-bro/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/orfjackal/territory-bro.git"
  },
  "dependencies": {
    "alphanum-sort": "1.0.2",
    "auth0-lock": "10.16.0",
    "axios": "0.16.1",
    "date-fns": "1.28.5",
    "history": "4.6.1",
    "jquery": "3.2.1",
    "js-cookie": "2.1.4",
    "lockr": "0.8.4",
    "lodash-es": "4.17.4",
    "openlayers": "4.1.1",
    "path-to-regexp": "1.7.0",
    "prop-types": "15.6.0",
    "purecss": "0.6.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-intl": "2.4.0",
    "react-redux": "5.0.5",
    "redux": "3.6.0",
    "redux-form": "6.7.0",
    "redux-logger": "3.0.6",
    "uuid": "3.0.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.5.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "4.0.0",
    "cross-env": "5.0.0",
    "css-loader": "0.28.3",
    "flow-bin": "0.47.0",
    "flow-coverage-report": "0.3.0",
    "flow-typed": "2.1.2",
    "glob": "7.1.2",
    "jsx-loader": "0.13.2",
    "mkdirp": "0.5.1",
    "mocha": "3.4.2",
    "source-map-explorer": "1.3.3",
    "style-loader": "0.18.1",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "engines": {
    "node": ">= 10.0.0",
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./web/js/test_setup.js 'web/js/**/*.test.js'",
    "autotest": "yarn run test --watch",
    "build": "cross-env NODE_ENV=production webpack --progress --colors -p",
    "build:langs": "babel-node --presets es2015 scripts/translator.js",
    "autobuild": "webpack --progress --colors --watch",
    "start": "webpack-dev-server --content-base web/html --host 0.0.0.0 --progress --colors",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "flow-coverage": "flow-coverage-report -i 'web/js/**/*.js' -t html -t text -o target/flow-coverage/",
    "analyze": "source-map-explorer target/webpack/bundle.js"
  }
}
