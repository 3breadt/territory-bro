{
  "name": "territory-bro",
  "version": "1.0.0-SNAPSHOT",
  "description": "Territory Bro is a tool for managing territory cards in the congregations of Jehovah's Witnesses.",
  "homepage": "https://territorybro.com",
  "author": "Esko Luontola (http://luontola.fi)",
  "license": "Apache-2.0",
  "bugs": "https://github.com/luontola/territory-bro/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/luontola/territory-bro.git"
  },
  "dependencies": {
    "@luontola/react-cache": "2.0.0-alpha.2019-05-02",
    "alphanum-sort": "1.0.2",
    "auth0-js": "9.10.2",
    "axios": "0.18.0",
    "core-js": "3",
    "date-fns": "1.30.1",
    "formik": "1.5.4",
    "history": "4.9.0",
    "jquery": "3.4.0",
    "js-cookie": "2.2.0",
    "lockr": "0.8.5",
    "lodash": "4.17.11",
    "ol": "5.3.2",
    "path-to-regexp": "3.0.0",
    "prop-types": "15.7.2",
    "purecss": "1.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-intl": "2.8.0",
    "react-redux": "7.0.3",
    "redux": "4.0.1",
    "redux-form": "8.2.0",
    "redux-logger": "3.0.6",
    "regenerator-runtime": "0.13.2",
    "uuid": "3.3.2",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "babel-loader": "8.0.5",
    "babel-plugin-react-intl": "3.0.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "chai": "4.2.0",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "flow-bin": "0.98.0",
    "flow-coverage-report": "0.6.1",
    "flow-typed": "2.5.1",
    "glob": "7.1.3",
    "jsx-loader": "0.13.2",
    "mkdirp": "0.5.1",
    "mocha": "6.1.4",
    "source-map-explorer": "1.8.0",
    "style-loader": "0.23.1",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1",
    "webpack-dev-server": "3.3.1"
  },
  "engines": {
    "node": ">= 10.0.0",
    "yarn": ">= 1.0.0"
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "build-react-cache": "git clone git@github.com:facebook/react.git --depth=1 && cd react && yarn install --frozen-lockfile && npm run build react-cache && cd .. && npm pack ./react/build/node_modules/react-cache && rm -rf react",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register --require ./web/js/test_setup.js 'web/js/**/*.test.js'",
    "autotest": "yarn run test --watch",
    "build": "webpack --mode=production --progress --colors -p",
    "build:langs": "babel-node --presets es2015 scripts/translator.js",
    "autobuild": "webpack --mode=development --progress --colors --watch",
    "start": "webpack-dev-server --mode=development --content-base web/html --host 0.0.0.0 --progress --colors",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "flow-coverage": "flow-coverage-report -i 'web/js/**/*.js' -t html -t text -o target/flow-coverage/",
    "analyze": "source-map-explorer target/webpack/bundle.js",
    "browserslist": "browserslist"
  }
}
