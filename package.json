{
  "name": "territory-bro",
  "version": "1.0.0-SNAPSHOT",
  "description": "Territory Bro is a tool for managing territory cards in the congregations of Jehovah's Witnesses.",
  "homepage": "https://territorybro.com",
  "author": "Esko Luontola (http://luontola.fi)",
  "license": "Apache-2.0",
  "bugs": "https://github.com/luontola/territory-bro/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/luontola/territory-bro.git"
  },
  "dependencies": {
    "alphanum-sort": "1.0.2",
    "auth0-lock": "11.11.0",
    "axios": "0.18.0",
    "date-fns": "1.29.0",
    "history": "4.7.2",
    "jquery": "3.3.1",
    "js-cookie": "2.2.0",
    "lockr": "0.8.5",
    "lodash-es": "4.17.11",
    "ol": "5.2.0",
    "path-to-regexp": "2.4.0",
    "prop-types": "15.6.2",
    "purecss": "1.0.0",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-intl": "2.7.2",
    "react-redux": "5.1.0",
    "redux": "4.0.1",
    "redux-form": "7.4.2",
    "redux-logger": "3.0.6",
    "uuid": "3.3.2",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-loader": "8.0.4",
    "babel-plugin-react-intl": "3.0.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "chai": "4.2.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "flow-bin": "0.85.0",
    "flow-coverage-report": "0.6.0",
    "flow-typed": "2.5.1",
    "glob": "7.1.3",
    "jsx-loader": "0.13.2",
    "mkdirp": "0.5.1",
    "mocha": "5.2.0",
    "source-map-explorer": "1.6.0",
    "style-loader": "0.23.1",
    "webpack": "4.24.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "engines": {
    "node": ">= 10.0.0",
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --require @babel/register --require ./web/js/test_setup.js 'web/js/**/*.test.js'",
    "autotest": "yarn run test --watch",
    "build": "webpack --mode=production --progress --colors -p",
    "build:langs": "babel-node --presets es2015 scripts/translator.js",
    "autobuild": "webpack --mode=development --progress --colors --watch",
    "start": "webpack-dev-server --mode=development --content-base web/html --host 0.0.0.0 --progress --colors",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "flow-coverage": "flow-coverage-report -i 'web/js/**/*.js' -t html -t text -o target/flow-coverage/",
    "analyze": "source-map-explorer target/webpack/bundle.js"
  }
}
